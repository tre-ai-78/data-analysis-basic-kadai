        "import numpy as np\n",
        "\n",
        "A = np.array([[0, 1], [2, 3], [4, 5]])\n",
        "B = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "\n",
        "matrix_mul = np.dot(A, B)\n",
        "print(matrix_mul)\n",
        "print(f'最大値 : {matrix_mul.max()}')"
